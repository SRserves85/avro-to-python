{%- block primitive %}
    def set_{{name}}(self, value: {{primitive_type_map[field.avrotype]}}) -> None:
        if isinstance(value, {{is_instance_primitive_type_map[field.avrotype]}}):
            self.{{name}} = value
        else:
            raise TypeError(f"field '{{name}}' should be type {{is_instance_primitive_type_map[field.avrotype]}} but was: {value}")

    def get_{{name}}(self) -> {{primitive_type_map[field.avrotype]}}:
        return self.{{name}}
{%- endblock -%}
